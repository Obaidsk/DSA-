#include <iostream>
using namespace std;

int main() {
    int n, search, found = 0;
    cout << "Enter number of students: ";
    cin >> n;

    int roll[n];
    cout << "Enter roll numbers:\n";
    for (int i = 0; i < n; i++) {
        cin >> roll[i];
    }

    cout << "Enter roll number to search: ";
    cin >> search;

    for (int i = 0; i < n; i++) {
        if (roll[i] == search) {
            cout << "Roll number found at index: " << i << endl;
            found = 1;
            break;
        }
    }
    if (!found) {
        cout << "Roll number not found." << endl;
    }

    return 0;
}
